                {
                    "organization_name": "orderer",
                    "orderer_name": "orderer.example.com"
                }
            ],
            "peer_specs": [
                {
                    "organization_name": "org0",
                    "peer_name": "peer0.org0.example.com"
                },
                {
                    "organization_name": "org0",
                    "peer_name": "peer1.org0.example.com"
                },
                {
                    "organization_name": "org1",
                    "peer_name": "peer0.org1.example.com"
                },
                {
                    "organization_name": "org1",
                    "peer_name": "peer1.org1.example.com"
                }
            ]
        }
    }
