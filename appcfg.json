{
    "host":"webserver",
    "port":"4000",
    "GOPATH":"../../source-artifacts",
    "kvs_path_prefix":"/tmp/Fabric_SampleWebApp/kvs/organization/",
    "cryptoSuite_path_prefix":"/tmp/Fabric_SampleWebApp/cryptoSuite/",
    "eventWaitTime":"30000",
    "channels": {
        "mychannel": {
            "configtx_path": "../../generated-artifacts/mychannel.tx",
            "chaincode": {
                "id": "mycc",
                "version": "v0",
                "path": "github.com/example_cc",
                "invoke_query_function_name":"invoke"
            },
            "channel_creator_spec": {
                "organization_name": "org0",
